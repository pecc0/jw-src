<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
	<settings defaultResolver="bm" />

	<typedef name="svn" classname="fm.last.ivy.plugins.svnresolver.SvnResolver" />

	<resolvers>
		<chain name="jw" returnFirst="true">
			<svn name="ivysvn" m2compatible="true" repositoryRoot="${svnRepositoryRoot}"
				userName="${svnUserName}" userPassword="${svnUserPassword}" binaryDiff="${svnBinaryDiff}">
				<ivy pattern="repository/[organisation]/[module]/[revision]/ivy.xml" />
				<artifact
					pattern="repository/[organisation]/[module]/[revision]/[artifact].[ext]" />
			</svn>

			<url name="jboss" m2compatible="true">
				<artifact
					pattern="http://repository.jboss.org/maven2/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>

			<!-- For xjavadoc 1.5, used in xdoclet -->
			<url name="tikalk" m2compatible="true">
				<artifact
					pattern="http://network.tikalk.com/release/repository/lunatech/[module]/[revision]/[artifact]-[revision].[ext]" />
			</url>

			<ibiblio name="ibiblio-maven2" m2compatible="true" />
			<ibiblio name="ibiblio" />
		</chain>
	</resolvers>

	<include url="${ivy.default.conf.dir}/ivysettings-public.xml" />
	<include url="${ivy.default.conf.dir}/ivysettings-shared.xml" />
	<include url="${ivy.default.conf.dir}/ivysettings-local.xml" />
	<include url="${ivy.default.conf.dir}/ivysettings-main-chain.xml" />
	<include url="${ivy.default.conf.dir}/ivysettings-default-chain.xml" />
</ivysettings>
